pipeline {
    agent any
    
    stages {
        stage('Build') {
            steps {
                echo "Building the web application"
            }
        }
        
        stage('Test') {
            steps {
                echo "Testing the web application"
            }
        }
        
        stage('Deploy') {
            steps {
                echo "Deploying the web application"
            }
        }
    }
}